<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
  <addon 
  id="{addonid}" 
  name="{name}" 
  version="{version}" 
  provider-name="{author}">
  <requires>
    <import addon="xbmc.addon" version="12.0.0"/>  
  </requires>
  <extension point="xbmc.addon.repository" name="{name}">
      <dir>
      <info compressed="true">{url}{output_path}addons.xml</info>
      <checksum>{url}{output_path}addons.xml.md5</checksum>
      <datadir zip="true">{url}{output_path}</datadir>
      <hashes>false</hashes>
  </dir>
  <dir minversion="19.9.9">
      <info compressed="true">{url}{output_path}nexus/addons.xml</info>
      <checksum>{url}{output_path}nexus/addons.xml.md5</checksum>
      <datadir zip="true">{url}nexus/{output_path}</datadir>
      <hashes>false</hashes>
  </dir>   
  <dir minversion="19.0.0">
      <info compressed="true">{url}{output_path}matrix/addons.xml</info>
      <checksum>{url}{output_path}matrix/addons.xml.md5</checksum>
      <datadir zip="true">{url}matrix/{output_path}</datadir>
      <hashes>false</hashes>
  </dir>    
  <dir minversion="17.0.0" maxversion="18.9.0">
      <info compressed="true">{url}{output_path}leia/addons.xml</info>
      <checksum>{url}{output_path}leia/addons.xml.md5</checksum>
      <datadir zip="true">{url}leia/{output_path}</datadir>
      <hashes>false</hashes>
  </dir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>{summary}</summary>
    <description>{description}</description>
    <disclaimer></disclaimer>
    <platform>all</platform>
    <assets>
      <icon>icon.png</icon>
      <fanart>fanart.jpg</fanart>
    </assets>
  </extension>
</addon>
